<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <application>every-cloud</application>
    <version>1</version>
	<threadsafe>true</threadsafe>
    <static-files>
	    <include path="/**.html" />
	    <include path="/**.js" />
	    <include path="/**.css" />
	    <include path="/**.jpg" />
	    <include path="/**.gif" />
	    <include path="/**.png" />
    </static-files>

    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
    </system-properties>
</appengine-web-app>